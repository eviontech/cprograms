#include<stdio.h>
int main()
{
    int a,b,c;
    scanf("%d %d %d",&a,&b,&c);
    if(a>b)
    {
        if(a>c)
        {
            printf("number a : %d is the largest number",a);
        }else{
            printf("number c : %d is the largest number",c);
        }
    }else{
        if(b>c)
        {
            printf("number b : %d is the largest number",b);
        }else{
            printf("number c : %d is the largest number",c);
        }
    }
}
